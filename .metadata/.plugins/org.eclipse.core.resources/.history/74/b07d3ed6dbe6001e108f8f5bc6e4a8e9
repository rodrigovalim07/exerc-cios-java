package entidades;

import java.time.Instant;
import java.util.Date;

import enums.StatusPedido;

public class Pedido {

	private Date momento;
	private StatusPedido status;
	
	public Pedido() {
	}

	public Pedido(Instant agora, String status) {
		this.momento = agora;
		this.status = status;
	}
	
	
}
